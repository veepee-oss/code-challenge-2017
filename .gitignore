# Symfony: Cache, logs & sessions
/app/cache/*
!app/cache/.gitkeep

/app/logs/*
!app/logs/.gitkeep

/var/cache/*
!/var/cache
!var/cache/.gitkeep

/var/logs/*
!/var/logs
!var/logs/.gitkeep

/var/sessions/*
!/var/sessions
!var/sessions/.gitkeep

# Parameters
/app/config/parameters.yml

# Managed by Composer
composer.phar
/app/bootstrap.php.cache
/var/bootstrap.php.cache
/bin/*
!bin/console
!bin/symfony_requirements
/var/*
!var/SymfonyRequirements.php
/vendor/

# Assets and user uploads
/web/bundles/

# PHPUnit
/app/phpunit.xml
/phpunit.xml

# Deploy
/deploy
!/deploy/deploy.sh
!/deploy/remote/install.sh

# Other
/supervisord*
.idea/
.DS_Store

